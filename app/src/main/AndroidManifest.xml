<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mad.mobiletimetable" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mad.mobiletimetable.ActivityMain"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mad.mobiletimetable.ActivitySettings"
            android:label="@string/title_activity_activity_settings" >
        </activity>
        <activity
            android:name="mad.mobiletimetable.ActivityFullTimetable"
            android:label="@string/title_activity_activity_full_timetable" >
        </activity>
        <activity
            android:name="mad.mobiletimetable.ActivityTimetable"
            android:label="@string/title_activity_activity_timetable" >
        </activity>
    </application>

</manifest>
